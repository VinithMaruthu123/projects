package org.yesbank;

import org.unionbank.UnionBank;

public class YesBank extends UnionBank {
    private void accountInfo(String name) {
		// TODO Auto-generated method stub
System.out.println("AC holder is :" + name);
	}
    private void accountInfo(int id) {
		// TODO Auto-generated method stub
    	System.out.println("AC  holder id is:"+id);

	}
	public static void main(String[] args) {
		YesBank y = new YesBank();
		y.accountInfo("maruthu");
		y.accountInfo(1234);
	}
	
	
	
}
